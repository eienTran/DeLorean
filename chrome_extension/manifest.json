{
  "name": "DeLorean",
  "description": "Time-travel debugger for Svelete applications",
  "version": "1.0",
  "manifest_version": 3,
  "devtools_page": "./devtools.html",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "debugger"],
  "action": {
    "default_icon": {
      "128": "./SvelteDeLorean.png"
    }
  },
  "icons": {
    "128": "./SvelteDeLorean.png"
  }
}
